    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^6.0.1",
    "vue-tsc": "^3.1.8"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.13.2",
    "element-plus": "^2.12.0",
    "pinia": "^3.0.4",
    "sass": "^1.96.0",
    "vue": "^3.5.13",
